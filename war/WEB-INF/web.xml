<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Sample</servlet-name>
		<servlet-class>biz.finder.SampleServlet</servlet-class>
	</servlet>	
	<servlet>
		<servlet-name>updatePointsTable</servlet-name>
		<servlet-class>biz.finder.ipl.UpdatePointsTableServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>updateFixturesTable</servlet-name>
		<servlet-class>biz.finder.ipl.UpdateFixturesTableServlet</servlet-class>
	</servlet>	
	<servlet>
		<servlet-name>listEntity</servlet-name>
		<servlet-class>biz.finder.ipl.get.ListServlet</servlet-class>
	</servlet>	
	<servlet>
		<servlet-name>predict</servlet-name>
		<servlet-class>biz.finder.ipl.get.PredictServlet</servlet-class>
	</servlet>		
	<servlet-mapping>
		<servlet-name>Sample</servlet-name>
		<url-pattern>/sample</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>updatePointsTable</servlet-name>
		<url-pattern>/ipl/updatePointsTable</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>updateFixturesTable</servlet-name>
		<url-pattern>/ipl/updateFixturesTable</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>listEntity</servlet-name>
		<url-pattern>/ipl/list</url-pattern>
	</servlet-mapping>	
	<servlet-mapping>
		<servlet-name>predict</servlet-name>
		<url-pattern>/ipl/predict</url-pattern>
	</servlet-mapping>	
	<welcome-file-list>
		<welcome-file>app.html</welcome-file >
	</welcome-file-list>
</web-app>
